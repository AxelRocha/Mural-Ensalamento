      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {
    "ensalar": {
      "accepts": [],
      "returns": {
        "type": "object",
        "root": true
      },
      "description": "ensala todos os eventos",
      "http": {
        "verb": "post",
        "path": "/ensalar"
      }
    }
  }
